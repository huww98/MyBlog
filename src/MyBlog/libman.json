{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "defaultDestination": "wwwroot/lib/",
    "libraries": [
        {
            "library": "jquery@3.3.1",
            "files": [ "jquery.js", "jquery.min.js" ]
        },
        {
            "library": "twitter-bootstrap@4.2.1",
            "files": [
                "css/bootstrap.css",
                "css/bootstrap.min.css",
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.min.js"
            ]
        },
        {
            "library": "bootstrap-fileinput@4.5.1",
            "destination": "wwwroot/lib/fileinput/",
            "files": [
                "css/fileinput.css",
                "css/fileinput.min.css",
                "js/fileinput.js",
                "js/fileinput.min.js",
                "js/locales/zh.js",
                "js/locales/zh.min.js",
                "themes/fa/theme.js",
                "themes/fa/theme.min.js",
                "img/loading.gif"
            ]
        },
        {
            "library": "bootstrap-treeview@1.2.0"
        },
        {
            "library": "font-awesome@4.7.0"
        },
        {
            "library": "jquery-validate@1.19.0",
            "files": [ "jquery.validate.js", "jquery.validate.min.js" ]
        },
        {
            "library": "jquery-validation-unobtrusive@3.2.11"
        },
        {
            "library": "masonry@4.2.2"
        },
        {
            "library": "remarkable@1.7.1"
        }
    ]
}
